{
  "manifest_version": 3,
  "name": "WxCC Desktop PWA Blocker",
  "version": "1.0",
  "description": "Identify and remove PWA manifest for WxCC Desktop",
  "content_scripts": [
  {
    "js": [ "content.js" ],
    "matches": ["https://desktop.wxcc-us1.cisco.com/*"],
    "run_at": "document_start"
  }],
  "icons": {
      "128": "wxcc128.png",
      "32": "wxcc32.png",
      "64": "wxcc64.png"
   }
}
